{
    "name": "Chat",
    "base": "PersistedModel",
    "idInjection": true,
    "options": {
        "validateUpsert": true
    },
    "properties": {
        "name": {
            "type": "string",
            "required": true
        }
    },
    "validations": [],
    "relations": {
        "members": {
            "type": "hasMany",
            "model": "User",
            "foreignKey": "userId",
            "description": "Участник чата"
        },
        "messages": {
            "type": "hasMany",
            "model": "Message",
            "foreignKey": "messageId",
            "description": "Сообщения чата"
        }
    },
    "acls": [],
    "methods": {}
}